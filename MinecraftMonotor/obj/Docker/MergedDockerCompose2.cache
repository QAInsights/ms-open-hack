services:
  minecraftmonotor:
    build:
      context: C:\SourceCode\Github-Public\MS-OpenHack-Containers\MinecraftMonotor
      dockerfile: MinecraftMonotor/Dockerfile
    entrypoint: tail -f /dev/null
    environment:
      NUGET_FALLBACK_PACKAGES: /root/.nuget/fallbackpackages
    image: minecraftmonotor:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages
        --additionalProbingPath /root/.nuget/fallbackpackages  bin/Debug/netcoreapp2.0/MinecraftMonotor.dll'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/bash -c "if PID=$$(pidof
        -x dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    volumes:
    - C:\SourceCode\Github-Public\MS-OpenHack-Containers\MinecraftMonotor\MinecraftMonotor:/app:rw
    - C:\Users\mails\vsdbg\vs2017u5:/remote_debugger:ro
    - C:\Program Files\dotnet\sdk\NuGetFallbackFolder:/root/.nuget/fallbackpackages:ro
    - C:\Users\mails\.nuget\packages:/root/.nuget/packages:ro
version: '3.4'